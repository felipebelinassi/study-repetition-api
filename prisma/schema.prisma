datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String @id @default(uuid()) @map("user_id")
  username      String
  email         String
  password      String
  createdAt     DateTime @default (now()) @map("created_at")

  @@map("user")
}