datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id            String @id @default(uuid()) @map("id")
  username      String @unique
  email         String @unique
  password      String
  createdAt     DateTime @default (now()) @map("created_at")

  @@map("user")
}